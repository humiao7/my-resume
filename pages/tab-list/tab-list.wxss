@import '../../common.wxss';

page{
    background-color: #FFFFFF;
    height: 100%;
}

.weui-tabs-bar__wrp {
    border-bottom: 1px solid #eee;
}

.weui-tabs-swiper {
    width: 100%;
    height: 1100rpx;
}

.weui-tabs-bar__title {
    margin: 0px 10px;
}

.tab-bar-title__selected {
    font-size: 20px;
    font-weight: bold;
}

.tab-content {
    height: 1100rpx;
    width: 100%;
    box-sizing: border-box;
    background: #efefef;
}

.tab-content .list{
   height: 100%;
   background: #fff;
}

.tab-content .list .item{
    padding: 20rpx 30rpx;
    border-bottom: 2rpx solid #efefef;
}

